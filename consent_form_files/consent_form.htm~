<script>
	function defineConsentTabSubmitHandler() {

	//http://jsfiddle.net/mapb_1990/hTPY7/9/ use this
		$('#btnSubmitConsent').on('click', function() {
			var isValidConsentFormTab = $('#formConsent').valid();
			if (isValidConsentFormTab) {
				$('#formConsent').submit();
			}
		});
	}

	function setConsentNotificationMessage(message) {
		var notificationDiv = $(".error-summary-top-consent")
		notificationDiv.removeClass("hide");
		notificationDiv.show();
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	function defineConsentFormValidator() {
		var validatorAppointmentTab = null;
		validatorAppointmentTab = $("#formConsent").bind(
				"invalid-form.validate",
				function() {
					$("#error-summary-bottom-appointment").html(
							"Your form contains " + validatorAppointmentTab.numberOfInvalids()
									+ " errors, see details above.");
				}).validate({
			errorContainer : $(".error-summary-top-consent"),
			rules : {
				consent_doctor_name : {
					required : true
				},
				witness : {
					required : true
				}
			},
			highlight : function(element) {
				$(element).closest('.form-control').addClass('has-error');
			},
			unhighlight : function(element) {
				$(element).closest('.form-control').removeClass('has-error');
			},

			errorElement : 'span',
			errorClass : 'alert-block',
			errorPlacement : function(error, element) {
				if (element.parent('.form-control').length) {
					error.insertAfter(element.parent());
				} else {
					error.insertAfter(element);
				}
			}
		});
	}

	function clearConsentForm() {
		$("#formConsent input:not([readonly])[type!='hidden']").val("");
		$("#treatment_signature_div").jSignature("reset");
		$("#drugs_signature_div").jSignature("reset");
		$("#changes_signature_div").jSignature("reset");
		$("#extract_signature_div").jSignature("reset");
		$("#crown_signature_div").jSignature("reset");
		$("#denture_signature_div").jSignature("reset");
		$("#canal_signature_div").jSignature("reset");
		$("#periodontal_signature_div").jSignature("reset");
		$("#fillings_signature_div").jSignature("reset");
		$("#pedodontics_signature_div").jSignature("reset");
		$("#user_signature_div").jSignature("reset");
		$("#dentist_signature_div").jSignature("reset");
		$("#witness_signature_div").jSignature("reset");
	}

	function defineConsentAjaxForm() {
		$('#formConsent').ajaxForm({
			success : function(responseText) {
				var patientId = $("#consentUserId").val();
				//Clear all the fields in the consent form except hidden fields
				clearConsentForm();
				//refresh medical and consent history
				refreshPatient(patientId);

				if (responseText.REASON) {
					setConsentNotificationMessage(responseText.REASON);
				} else {
					setConsentNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setConsentNotificationMessage("Error occurred. Please try again later.");
			},
			beforeSerialize : function() {
				var signatureDivs = $(".signature");
				for (var index = 0; index < signatureDivs.length; index++){
					try {
						var sigDiv = signatureDivs[index];
						var $sigDiv = $(sigDiv);
						var sigDivValue = "data:image/jsignature;base30," + $sigDiv.jSignature('getData', "base30")[1];
						$("#" + $sigDiv.attr("attached_input")).val(sigDivValue);
					} catch (err){
						/*Ignore*/
					}
				}
			}
		});
	}

	function setPatientConsentInfo(patientId) {
		var patient = patientProfiles[patientId];
		$("#consentUserId").val(patientId);
		var patientUserProfile = patient.userProfile;
		$("#consent_patient_name").val(patientUserProfile.name);
	}
	
	function defineConsentSignature(){
		var $sigdivPatient = $(".signature");
		$sigdivPatient.jSignature({
			'height' : 70,
			'width' : 300
		})
		
		$(".clear_signature").click(function(){
			var attached_div = $("#" + $(this).attr("attached_input"));
			attached_div.jSignature('reset');
		})
	}

	$(document).ready(function() {
		//$('.collapseOtherPanels').click(function () {
		//    $('#accordion .in').collapse('hide');
		//});
		defineConsentAjaxForm();
		defineConsentFormValidator();
		defineConsentTabSubmitHandler();
		defineConsentSignature();
	});
</script>

	</div>
	<div class="tab-pane" id="tabUSERHISTORY">
		




<div id="user_history">
	<input name="consent_user_id" id="consentUserId" type="hidden">
	<div class="row row-centered topbuffer">
		<div>
			<div class="panel panel-primary" id="inbox_container">
				<div class="panel-heading text-center">
					<h3 class="panel-title">Medical History</h3>
				</div>
				<div class="panel-body">
					<table id="medicalHistoryLogTable" style="width:100%; table-layout: fixed; margin-top: 10px;"></table>
				</div>
			</div>
			<div class="panel panel-primary" id="inbox_container">
				<div class="panel-heading text-center">
					<h3 class="panel-title">Consent History</h3>
				</div>
				<div class="panel-body">
					<table id="consentHistoryTable" style="width:100%; table-layout: fixed; margin-top: 10px;"></table>
				</div>
			</div>
		</div>
	</div>
</div>


