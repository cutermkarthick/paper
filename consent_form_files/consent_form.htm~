						<p class="form-control-static text-left" id="viewAppointmentStatus"></p>
					</div>
				</div>

				<div class="topbottombuffer">
					<div class="btn-toolbar">
						<a class="btn btn-success" id="confirm_appointment">Confirm</a> <a class="btn btn-success" id="reject_appointment">Cancel</a> <a class="btn btn-default" id="close_button" data-dismiss="modal">Close</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>


<script>
	var applicationContext = '/portal';
	var dentistId = '2';

	function defineAppointmentActions() {
		$("#reject_appointment").click(function() {
			event_id = $(this).attr("event_id");
			value = confirm('Are you sure you want to cancel the event');
			if (value) {
				cancelEvent(event_id);
				//location.reload(true);
				//reloadNewAppointmentRequestTable();
				showNewAppointmentsDataTable();
			}
		});
		$("#confirm_appointment").click(function() {
			event_id = $(this).attr("event_id");
			confirmEvent(event_id);
			//location.reload(true);
			//reloadNewAppointmentRequestTable();
			showNewAppointmentsDataTable();
		});
	}
	
	/*$('#newAppointmentRequestTable tbody').on('click', 'tr', function (e) {
		$(this).addClass('clicked');
	});*/
	
	function getNewAppointmentsInArray(newAppointments, reload){
		var dataSet = new Array();
		if(newAppointments){
			var indexCounter = 1;
			for (var index in newAppointments){
				var appointment = newAppointments[index];
				var appointmentId = appointment.id;
				var displayName = appointment.patient;
				var date = appointment.start;
				var purpose = appointment.purpose;
				var reason = appointment.remarks;
				
				var dataSetElem = [appointmentId, indexCounter, displayName, date, purpose, reason];
				dataSet.push(dataSetElem);
				indexCounter++;
			}
		}
		return dataSet;
	}
	
	var dataTableInstanceAppointments = null;
	function showNewAppointmentsDataTable(){
		var apptTable = $("#newAppointmentRequestTable");
		var ajaxUrl = '/portal/dentists/pending';
		var dentistId = $("#dentistId").val();
		var data = {dentistId : dentistId};
		var dataSet = getNewAppointmentsInArray(newAppointmentRequests);
		if(!$.fn.dataTable.isDataTable('#newAppointmentRequestTable')){
			var successCallback = function(response){
				if(response.SUCCESS){
					var newAppointments = response.Appointments.replace(/[\\*]/g,"");
					var jsonAppointment = JSON.parse(newAppointments);
					dataSet = getNewAppointmentsInArray(jsonAppointment);
					dataTableInstanceAppointments.clear();
					if(dataSet.length > 0){
						for(var i=0; i< dataSet.length; i++){
							dataTableInstanceAppointments.row.add(dataSet[i]);
						}
					}
					dataTableInstanceAppointments.draw();
					reloadNewRequestBadge(dataSet);
				} else {
					//alert("Error occurred!");
				}
			};
			dataTableInstanceAppointments = apptTable.DataTable({
				"data": dataSet,
				"deferRender" : true,
				"ajax": {"url" : ajaxUrl, "data" : data, "type" : "POST", "success" : successCallback},
			 	"columnDefs":[
			 	              	{"visible" : false, "targets" : [0, 5]}            
				 			 ]  
			});
			//reloadUnreadBadge();
		} else {
			
			dataTableInstanceAppointments.ajax.reload();
		}
		
	}
	
	function reloadNewRequestBadge(table_data){
		try{
			//newReq_count = 0
			//table_data = $("#newAppointmentRequestTable").DataTable().ajax.json().data;
			if (table_data != null){
				$("#appointment_badge").text(table_data.length);
				$("#appointment_request_badge").text(table_data.length);
			} else {
				$("#appointment_badge").text(0);
				$("#appointment_request_badge").text(0);
			}
			} 
		catch(err){
			/* Ignore */
		}
	}
	
	
	function setAppointmentNotificationMessage(message) {
		var notificationDiv = $(".appointment_notification")
		notificationDiv.removeClass("hide");
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	function defineAppointmentTabSubmitHandler() {
		$('#btnRequestAppt').on('click', function() {
			$('#new_appointment_form').submit();
		});
	}

	function defineAppointmentDatePicker() {
		$("#appointment_date_input").blur(function(){
			getFreeSlots();
		})
	}

	function getFormattedDate(date) {
		var year = date.getFullYear();
		var month = (1 + date.getMonth()).toString();
		month = month.length > 1 ? month : '0' + month;
		var day = date.getDate().toString();
		day = day.length > 1 ? day : '0' + day;
		return month + '/' + day + '/' + year;
	}

	function defineSlotOnChange() {
		var TIME_SLOT = 30 * 60 * 1000;
		var MILLISECOND_IN_ONE_HOUR = 60 * 60 * 1000;
		$("#slot").change(
				function() {
					var startTime = $("#slot").val();
					if (startTime) {
						var dateString = $("#appointment_date_input").val();
						var splitDate = dateString.split("/")
						var hours = parseInt(startTime.split(":")[0]);
						var minutes = parseInt(startTime.split(":")[1]);
						var startDate = new Date(parseInt(splitDate[2]), parseInt(splitDate[0]) - 1,
								parseInt(splitDate[1]), hours, minutes);
						var endDate = new Date(parseInt(splitDate[2]), parseInt(splitDate[0]) - 1,
								parseInt(splitDate[1]), hours, minutes + 29);
						$("#start_time").val(startDate.getTime())
						$("#end_time").val(endDate.getTime())
					}
				});
	}

	function getFreeSlots() {
		var dentistId = $("#dentistId").val();
		var date = $("#appointment_date_input").val();
		var location = $("#location").val();

		var data = {
			dentistId : dentistId,
			date : date,
			clinic_location : location
		};
		$.ajax({
			url : applicationContext + '/appointment/freeslots',
			data : data,
			type : "POST"

		}).success(function(response) {
			slots = response;
			$slotDiv = $("#slot");
			$slotDiv.empty();
			for (index in slots) {
				$slotDiv.append($('<option value="' + slots[index] + '">' + slots[index] + '</option>'))
			}
			$("#slot").change();
		});
	}

	function cancelEvent(id) {
		var data = {
			id : id
		}
		$.ajax({
			url : applicationContext + '/appointment/cancel',
			data : data,
			type : "POST"

		}).success(function(response) {
			$("#calendar").fullCalendar('refetchEvents')
			$("#appointment-modal-sm").modal('hide')
			if (response.REASON) {
				setAppointmentNotificationMessage(response.REASON);
			} else {
				setAppointmentNotificationMessage("Error occurred. Please try again later.");
			}
		}).error(function(responseText) {
			setAppointmentNotificationMessage("Error occurred. Please try again later.");
		});
	}

	function confirmEvent(id) {
		var data = {
			id : id
		}
		$.ajax({
			url : applicationContext + '/appointment/confirm',
			data : data,
			type : "POST"

		}).success(function(response) {
			$("#calendar").fullCalendar('refetchEvents')
			$("#appointment-modal-sm").modal('hide')
			if (response.REASON) {
				setAppointmentNotificationMessage(response.REASON);
			} else {
				setAppointmentNotificationMessage("Error occurred. Please try again later.");
			}
		}).error(function(responseText) {
			setAppointmentNotificationMessage("Error occurred. Please try again later.");
		});
	}

	function fetchAppointment(id) {
		$.ajax({
			url : applicationContext + '/appointment/fetch/' + id,
			data : {},
			type : "GET"
		}).success(function(response) {
			if (response.SUCCESS) {
				appointmentJson = response.DETAILS;

				$("#viewAppointmentPatient").html(appointmentJson.patient)
				$("#viewAppointmentTime").html(appointmentJson.time)
				$("#viewAppointmentLocation").html(appointmentJson.location)
				if(appointmentJson.purpose == null)
					$("#viewAppointmentPurpose").html("OTHER")
				else
					$("#viewAppointmentPurpose").html(appointmentJson.purpose)
				$("#viewAppointmentRemarks").html(appointmentJson.remarks)
				if (appointmentJson.status == "CONFIRMED") {
					status = "Confirmed";
				}
				else if (appointmentJson.status == "COMPLETED") {
					status = "Completed";
				}
				else
					status = "Awaiting Confirmation";
				$("#viewAppointmentStatus").html(status)

				$("#appointment-modal-sm").modal()
				$("#confirm_appointment").attr("event_id", id)
				if (appointmentJson.status == "CONFIRMED" || appointmentJson.status == "COMPLETED") {
					$("#confirm_appointment").addClass("hide");
				} else {
					$("#confirm_appointment").removeClass("hide");
				}

				$("#reject_appointment").attr("event_id", id)
			} else {
				setAppointmentNotificationMessage("Error occurred. Please try again later.");
			}
		}).error(function(responseText) {
			setAppointmentNotificationMessage("Error occurred. Please try again later.");
		});
	}

	function defineAppointmentCalendar() {
		var calendar;

		$(document).on('shown.bs.tab', 'a[data-toggle="tab"]', function(e) {
			if (e.target.id == "my_calendar_link") {
				if (calendar) {
					$("#calendar").fullCalendar('refetchEvents')
				} else {
					calendar = $('#calendar').fullCalendar({
						header : {
							left : 'prev,next today',
							center : 'title',
							right : 'month,agendaWeek,agendaDay'
						},
						selectable : false,
						selectHelper : false,
						defaultView : 'agendaDay',
						editable : false,
						events : {
							url : applicationContext + '/appointment/feed',
							cache : false,
							data : {
								'dentistId' : dentistId
							},
							method : "POST"
						},
						eventClick : function(calEvent, jsEvent, view) {
							fetchAppointment(calEvent.id)
						},
						lazyFetching : false
					});
				}

			}
		})
	}

	function defineAppointmentValidator() {
		var validatorAppointmentTab = null;
		validatorAppointmentTab = $("#new_appointment_form").bind(
				"invalid-form.validate",
				function() {
					$("#error-summary-bottom-appointment").html(
							"Your form contains " + validatorAppointmentTab.numberOfInvalids()
									+ " errors, see details above.");
				}).validate({
			errorContainer : $("#error-summary-bottom-appointment"),
			rules : {
				appointmentPatientName : {
					required : true
				},
				appointment_date_input : {
					required : true,
					customdatevalidator : true,
					futuredatevalidator : true
				},
				slot : {
					required : true
				}
			},
			messages : {
				appointment_date_input : "Enter the date of appointment",
				slot : "Slots are not available for the selected date. Please select another date."
			},
			highlight : function(element) {
				$(element).closest('.form-control').addClass('has-error');
			},
			unhighlight : function(element) {
				$(element).closest('.form-control').removeClass('has-error');
			},

			errorElement : 'span',
			errorClass : 'alert-block',
			errorPlacement : function(error, element) {
				if (element.parent('.form-control').length) {
					error.insertAfter(element.parent());
				} else {
					error.insertAfter(element);
				}
			}
		});
	}

	function setComposePatients(allPatients, $select) {
		for ( var index in allPatients) {
			var patient = allPatients[index];
			$select.append($("<option value='" + patient.id + "'>" + patient.name + " </option>"));
		}
	}
	
	/*
	 FUNCTION TO SET THE 'BOOTSTRAP v3 DATATABLES.NET' PLUGIN BASED TABLE
	 Set the data in client side javascript array and then render the bootstrap3 datatable. 
	 Refer http://www.datatables.net/examples/data_sources/js_array.html 
	 START: This is for rendering the NEW APPOINTMENT REQUESTS  datatable
	*/
	function setNewAppointmentRequestTable(newAppointments, newPatientsTableId){
		var dataSet = new Array();
		if(newAppointments){
			var indexCounter = 1;
			for (var index in newAppointments){
				var appointment = newAppointments[index];
				var appointmentId = appointment.id;
				var displayName = appointment.patient;
				var date = appointment.start;
				var purpose = appointment.purpose;
				var reason = appointment.remarks;
				
				var dataSetElem = [appointmentId, indexCounter, displayName, date, purpose, reason];
				dataSet.push(dataSetElem);
				indexCounter++;
			}
		}
		
		newPatientsTableId.dataTable({
			"data": dataSet,
			"deferRender" : true,
		 	"columnDefs":[
		 	              	{"visible" : false, "targets" : [0, 5]}            
			 			 ],
		});
	}
	
	
	
	function defineAppointmentRequestClick(){
		$('#newAppointmentRequestTable tbody').on('click', 'tr', function (e) {
			var appointmentDataTable = $('#newAppointmentRequestTable').DataTable();
			var appointmentData = appointmentDataTable.row(this).data();
			var appointmentId = appointmentData[0];
			
			
			for(var j=0;j<4;j++){
				$('td', this).eq(j).removeClass('highlight');
				$('td', this).eq(j).addClass('unhighlight');
			}		
			
			fetchAppointment(appointmentId);
		}); 
	}
	
	
	$(document).ready(function() {
		defineAppointmentCalendar();
		defineAppointmentDatePicker();
		defineSlotOnChange();
		defineAppointmentValidator();
		defineAppointmentTabSubmitHandler();
		defineAppointmentActions();
		defineAppointmentRequestClick();
		$('#new_appointment_form').ajaxForm({
			success : function(responseText) {
				$("#appointment_date_input").val("")
				$("#remarks").val("")
				$("#slot").empty()
				$('#collapseThree a:eq(0)').tab('show')

				if (responseText.REASON) {
					setAppointmentNotificationMessage(responseText.REASON);
				} else {
					setAppointmentNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setAppointmentNotificationMessage("Error occurred. Please try again later.");
			}
		});
		// Set the timezone
		$("#timezone").val(jstz.determine().name());
		$("#disclaimer_popover").popover();
	})
</script>

		</div>
	</div>
	
</div>


<script>

	
	var dentistId = '2';
	
	
