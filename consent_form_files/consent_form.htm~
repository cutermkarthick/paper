					<div class="checkbox">
						<label id="anticoagulant_label"><input type="checkbox" name="anticoagulant" id="anticoagulant" value="anticoagulant">Anticoagulants (Blood
							thinners)</label>
					</div>
				</div>
			</div>
			<div class="row row-centered topbuffer">
				<div class="col col-md-6 col-xs-6 col-centered col-top">
					<p id="other_medicine_label">Other</p>
				</div>
				<div class="col col-md-6 col-xs-6 col-centered col-top">
					<input type="text" name="other_medicine" id="other_medicine" class="form-control input-sm">
				</div>
			</div>
		</div>
		
	</div>
	
	
	
