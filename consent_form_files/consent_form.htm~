</div>
</form>

<script type="text/javascript">
	function setPatientEmergencyInfo(patientId) {
		var patient = patientProfiles[patientId];
		var patientEmergencyProfile = patient.emergencyProfile;
		if (patientEmergencyProfile) {
			$("#emergencyFirstName").html(patientEmergencyProfile.emergencyFirstName);
			$("#emergencyMiddleName").html(patientEmergencyProfile.emergencyMiddleName);
			$("#emergencyLastName").html(patientEmergencyProfile.emergencyLastName);
			$("#emergencyCellPhones").html(patientEmergencyProfile.emergencyCellPhones);
			$("#emergencyWorkPhones").html(patientEmergencyProfile.emergencyWorkPhones);
			$("#emergencyHomePhones").html(patientEmergencyProfile.emergencyHomePhones);
			$("#emergencyEmail").html(patientEmergencyProfile.emergencyEmail);
			$("#emergencyRelation").html(patientEmergencyProfile.emergencyRelation);
		} else {
			$("#emergencyFirstName").html("");
			$("#emergencyMiddleName").html("");
			$("#emergencyLastName").html("");
			$("#emergencyCellPhones").html("");
			$("#emergencyWorkPhones").html("");
			$("#emergencyHomePhones").html("");
			$("#emergencyEmail").html("");
			$("#emergencyRelation").html("");
		}
	}
</script>
