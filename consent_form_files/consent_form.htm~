	<div class="panel panel-primary">
		<div class="panel-heading text-center">
			<h3 class="panel-title">Patient Signature</h3>
		</div>
		<div class="panel-body">
			<div class="form-group">
				<label id="patient_signature_label" class="col-sm-6 control-label">I have filled out this health questionnaire completely. I have advised the doctor
					of all medical problems that I am aware of.</label>
				<div class="col-sm-5" id="patientSignature"><div style="padding:0 !important; margin:0 !important;width: 100% !important; height: 0 !important; -ms-touch-action: none;margin-top:-1em !important; margin-bottom:1em !important;"></div><canvas class="jSignature" width="300" height="70" style="margin: 0px; padding: 0px; border: none; height: 70px; width: 300px; background-color: rgb(245, 245, 245);"></canvas><div style="padding:0 !important; margin:0 !important;width: 100% !important; height: 0 !important; -ms-touch-action: none;margin-top:-1.5em !important; margin-bottom:1.5em !important; position: relative;"></div></div>
			</div>
			<div class="row row-centered topbottombuffer">
				<div class="col-xs-12 col-md-8  col-centered">
					<span class="alert-block" id="error-summary-bottom-medical"></span>
					<div class="row row-centered spaced">
						<div class="col col-centered">
							<div class="btn-toolbar">
								<a class="btn btn-success btn-lg" id="btnClearPatientSignature">Clear Signature</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="panel panel-primary">
		<div class="panel-heading text-center">
			<h3 class="panel-title">Dentist Signature</h3>
		</div>
		<div class="panel-body">
			<div class="form-group">
				<label id="dentist_signature_label" class="col-sm-6 control-label">I have reviewed the Patient's health history form above.</label>
				<div class="col-sm-5" id="dentistSignature"><div style="padding:0 !important; margin:0 !important;width: 100% !important; height: 0 !important; -ms-touch-action: none;margin-top:-1em !important; margin-bottom:1em !important;"></div><canvas class="jSignature" width="300" height="70" style="margin: 0px; padding: 0px; border: none; height: 70px; width: 300px; background-color: rgb(245, 245, 245);"></canvas><div style="padding:0 !important; margin:0 !important;width: 100% !important; height: 0 !important; -ms-touch-action: none;margin-top:-1.5em !important; margin-bottom:1.5em !important; position: relative;"></div></div>
			</div>
			<div class="row row-centered topbottombuffer">
				<div class="col-xs-12 col-md-8  col-centered">
					<span class="alert-block" id="error-summary-bottom-medical"></span>
					<div class="row row-centered spaced">
						<div class="col col-centered">
							<div class="btn-toolbar">
								<a class="btn btn-success btn-lg" id="btnClearDentistSignature">Clear Signature</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


	<div class="alert alert-danger alert-dismissable hide medical_notification"></div>

	
	
	<div class="row row-centered topbottombuffer">
		<div class="col-xs-12 col-md-8  col-centered">
			<span class="alert-block" id="error-summary-bottom-medical"></span>
			<div class="row row-centered spaced">
				<div class="col col-centered">
					<div class="btn-toolbar">
						<a class="btn btn-success btn-lg" id="btnDoneMedical"><i class="fa fa-check"></i>Save</a>
						<a class="btn btn-success btn-lg disabled" id="btnAcceptMedical">Accept</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</form>

<script>
	function setPatientMedicalHistory(patientHistoryString, $div) {
		var patient_history = JSON.parse(patientHistoryString);
		$("#completed", $div).attr("disabled", false);
		$("p", $div).removeClass("medicalAlert")
		$("label", $div).removeClass("medicalAlert")
		for (condition in patient_history) {
			if (patient_history[condition] == "Submit") {
			} else if (patient_history[condition] == "yes") {
				$("#" + condition + "_yes", $div).attr("checked", "checked");
				if (condition != "good_health") {
					$("#" + condition + "_label", $div).addClass("medicalAlert");
				} else {
					$("#" + condition + "_label", $div).removeClass("medicalAlert");
				}
				//$("#" + condition + "_yes").attr("disabled", true);
			} else if (patient_history[condition] == "no") {
				$("#" + condition + "_no", $div).attr("checked", "checked");
				if (condition != "good_health") {
					$("#" + condition + "_label", $div).removeClass("medicalAlert");
				} else {
					$("#" + condition + "_label", $div).addClass("medicalAlert");
				}
				$("#" + condition + "_no", $div).attr("disabled", true);
			} else if (patient_history[condition] == condition) {
				$("#" + condition, $div).attr("checked", true);
				$("#" + condition + "_label", $div).addClass("medicalAlert");
				$("#" + condition, $div).attr("disabled", true);
			} else {
				$("#" + condition, $div).val(patient_history[condition]);
				$("#" + condition, $div).attr("title", patient_history[condition]);
				if (condition != "review_text") {
					$("#" + condition, $div).attr("disabled", true);
				}
			}
		}
		if ((patient_history["medication_list"] != null && patient_history["medication_list"] != "")) {
			$("#medication_yes", $div).attr("checked", "checked");
			$("#medication_no", $div).removeAttr("checked");
			$("#medication_label", $div).addClass("medicalAlert");
			showHideDiv('any_drug_medication_div', true);
		}
		if (patient_history["other_allergic"]) {
			$("#other_allergic_label", $div).addClass("medicalAlert");
		}
		if (patient_history["anything_else"]) {
			$("#anything_else_label", $div).addClass("medicalAlert");
		}

		$("#patientForm_Medical input").attr("disabled", false);
	}

	function setMedicalNotificationMessage(message) {
		var notificationDiv = $(".medical_notification");
		notificationDiv.removeClass("hide");
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	function defineMedicalButtons() {
		$('#btnDoneMedical').on('click', function() {
			$('#patientForm_Medical').submit();
		});
		$('#btnDoneReview').on('click', function() {
			$('#patientForm_Review').submit();
		});

	}

	function defineSignature() {
		var $sigdivPatient = $("#patientSignature");
		$sigdivPatient.jSignature({
			'height' : 70,
			'width' : 300
		});

		$("#btnClearPatientSignature").click(function() {
			$sigdivPatient.jSignature("reset");
		});

		var $sigdivDentist = $("#dentistSignature");
		$sigdivDentist.jSignature({
			'height' : 70,
			'width' : 300
		});

		$("#btnClearDentistSignature").click(function() {
			$sigdivDentist.jSignature("reset");
		});

		$("#btnAcceptMedical").click(function() {
			patientData = "data:image/jsignature;base30," + $("#patientSignature").jSignature('getData', "base30")[1];
			dentistData = "data:image/jsignature;base30," + $("#dentistSignature").jSignature('getData', "base30")[1];
			userId = $(".medical_userId").val();
			var data = {
				patientSignature : patientData,
				dentistSignature : dentistData,
				userId : userId
			}
			$.ajax({
				url : applicationContext + '/dentists/accept',
				data : data,
				type : "POST"

			}).success(function(responseText) {
				if (responseText.SUCCESS) {
					enableSignatureDivs();
					$("#patientSignature").jSignature("reset");
					$("#dentistSignature").jSignature("reset");
					var patient = patientProfiles[userId];
					var patientMedicalProfile = patient.medicalprofile;
					if (patientMedicalProfile) {
						patientMedicalProfile.patientSignature = patientData;
						patientMedicalProfile.dentistSignature = dentistData;
					}

					if (responseText.REASON) {
						setMedicalNotificationMessage(responseText.REASON);
					} else {
						setMedicalNotificationMessage("Your medical form has been accepted.");
					}
					//refreshes medical and consent history
					refreshPatient(userId);
				} else {
					setMedicalNotificationMessage("Error occurred. Please try again later.");
				}
			})
		});
		
		
	}

	$(document).ready(function() {
		defineMedicalButtons();
		defineSignature();
		defineMedicalLinkedRadioInput();

		$('#patientForm_Medical').ajaxForm({
			success : function(responseText) {
				if (responseText.SUCCESS) {
					if (responseText.REASON) {
						setMedicalNotificationMessage(responseText.REASON);
					} else {
						setMedicalNotificationMessage("Medical form has been updated successfully.");
					}
					//refreshes medical and consent history
					var userId = $(".medical_userId").val();
					refreshPatient(userId);
				} else {
					setMedicalNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setMedicalNotificationMessage("Error occurred. Please try again later.");
			}
		});
		
		$('#patientForm_Review').ajaxForm({
			success : function(responseText) {
				if (responseText.SUCCESS) {
					if (responseText.REASON) {
						setMedicalNotificationMessage(responseText.REASON);
					} else {
						setMedicalNotificationMessage("Your review has been updated successfully.");
					}
				} else {
					setMedicalNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setMedicalNotificationMessage("Error occurred. Please try again later.");
			}
		});
	});

	function disableSignatureDivs() {
		$('#btnClearPatientSignature').addClass("disabled");
		$('#btnClearDentistSignature').addClass("disabled");
		$('#btnAcceptMedical').addClass("disabled");
		$("#patientSignature").jSignature("disable");
		$("#dentistSignature").jSignature("disable");
	}

	function enableSignatureDivs() {
		$('#btnAcceptMedical').removeClass("disabled");
		$('#btnClearPatientSignature').removeClass("disabled");
		$('#btnClearDentistSignature').removeClass("disabled");
		$("#patientSignature").jSignature("enable");
		$("#dentistSignature").jSignature("enable");
	}
	
	function defineMedicalLinkedRadioInput() {
		
			$('input[name=currently_physician]').click(function() {
					var selectedValue = $('input[name=currently_physician]:checked').val();
					showHideDiv('what_is_being_treated_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=illness]').click(function() {
					var selectedValue = $('input[name=illness]:checked').val();
					showHideDiv('what_was_problem_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=blood_transfusion]').click(function() {
					var selectedValue = $('input[name=blood_transfusion]:checked').val();
					showHideDiv('had_blood_transfusion_div', ((selectedValue == "yes") ? true : false));
				});
		
			//any_drug_medication_div
		
			$('input[name=medication]').click(function() {
					var selectedValue = $('input[name=medication]:checked').val();
					showHideDiv('any_drug_medication_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=adverse_reaction_dental]').click(function() {
					var selectedValue = $('input[name=adverse_reaction_dental]:checked').val();
					showHideDiv('adverse_reaction_explain_dental_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=adverse_reaction_medical]').click(function() {
					var selectedValue = $('input[name=adverse_reaction_medical]:checked').val();
					showHideDiv('adverse_reaction_explain_medical_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=other_illness]').click(function() {
					var selectedValue = $('input[name=other_illness]:checked').val();
					showHideDiv('mental_health_other_illnes_div', ((selectedValue == "yes") ? true : false));
				});
		
			
		
			$('input[name=biophosphonates]').click(function() {
					var selectedValue = $('input[name=biophosphonates]:checked').val();
					showHideDiv('biophosphonates_explain_div', ((selectedValue == "yes") ? true : false));
				});
		
			}

	function setPatientMedicalInfo(patientId) {
		$("#patientForm_Medical input:text").val("");
		var patient = patientProfiles[patientId];
		var patientMedicalProfile = patient.medicalprofile;

		if (patientMedicalProfile) {
			setPatientMedicalHistory(JSON.stringify(patientMedicalProfile), $("#patientForm_Medical"));
			$('#btnDoneReview').removeClass("disabled");
			$('#btnDoneMedical').removeClass("disabled");
			try {
				//if (patientMedicalProfile.patientSignature) {
				//	$("#patientSignature").jSignature("setData", patientMedicalProfile.patientSignature);
				//} else {
					$("#patientSignature").jSignature("reset");
				//}
				//if (patientMedicalProfile.dentistSignature) {
				//	$("#dentistSignature").jSignature("setData", patientMedicalProfile.dentistSignature);
				//} else {
					$("#dentistSignature").jSignature("reset");
				//}

				if (patientMedicalProfile.form_status == "ACCEPT") {
					disableSignatureDivs();
				} else {
					enableSignatureDivs();
				}
			} catch (err) {
				//ignore
			}

		} else {
			$("#patientForm_Medical input").attr("disabled", false);
			$("#patientSignature").jSignature("reset");
			$("#dentistSignature").jSignature("reset");
			$('#btnDoneReview').removeClass("disabled");
			$('#btnDoneMedical').removeClass("disabled");
			$('#btnAcceptMedical').addClass("disabled");
			$('#btnClearPatientSignature').removeClass("disabled");
			$('#btnClearDentistSignature').removeClass("disabled");
		}
		$(".medical_userId").val(patientId);
		$(".medical_userId").attr("disabled", false);
	}
</script>
	</div>
