	$("#btnBriefView").on('click', function() {
		var patientId = $(this).attr("patientId");
		hideAllDivs();
		$("#patient_brief_view").removeClass("hide");
		setPatientBriefView(patientId);
		history.pushState(null, null, "#briefView");
	})

	$('#btnBackToAllPatientsTable2').on('click', function() {
		hideAllDivs();
		$('#dentist_dashboard').removeClass("hide");
		tabHref = $("ul#collapseOne li.active a").attr("href")
		history.pushState(null, null, tabHref);
	});
</script>

