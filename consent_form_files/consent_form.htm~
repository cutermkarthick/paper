<script>
	function timeConverter(UNIX_timestamp) {
		var timestamp = parseInt(UNIX_timestamp);
		var a = new Date(timestamp);
		var months = [ 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec' ];
		var year = a.getFullYear();
		var month = months[a.getMonth()];
		var date = a.getDate();
		var hour = a.getHours();
		var min = a.getMinutes();
		var sec = a.getSeconds();
		var time = date + ',' + month + ' ' + year + ' ' + hour + ':' + min + ':' + sec;
		return time;
	}

	function setPatientMedicalHistoryLog(patient_history, $div) {
		$("#patientForm_Medical input", $div).val("")
		$("#patientForm_Medical input", $div).attr("disabled", true)
		$("#completed", $div).attr("disabled", false);
		for (condition in patient_history) {
			if (patient_history[condition] == "Submit") {
			} else if (patient_history[condition] == "yes") {
				$("#" + condition + "_yes", $div).attr("checked", "checked");
				if (condition != "good_health") {
					$("#" + condition + "_label", $div).addClass("medicalAlert");
				} else {
					$("#" + condition + "_label", $div).removeClass("medicalAlert");
				}
			} else if (patient_history[condition] == "no") {
				$("#" + condition + "_no", $div).attr("checked", "checked");
				if (condition != "good_health") {
					$("#" + condition + "_label", $div).removeClass("medicalAlert");
				} else {
					$("#" + condition + "_label", $div).addClass("medicalAlert");
				}
			} else if (patient_history[condition] == condition) {
				$("#" + condition, $div).attr("checked", true);
				$("#" + condition + "_label", $div).addClass("medicalAlert");
			} else {
				$("#" + condition, $div).val(patient_history[condition]);
				$("#" + condition, $div).attr("title", patient_history[condition]);
				if (condition != "review_text")
					$("#" + condition, $div).attr("disabled", true);
			}
		}
	}

	function setPatientUserHistory(patientId) {
		var patient = patientProfiles[patientId];
		var patientConsentHistory = patient.consentHistory;
		$("#consentHistoryTable").empty();
		if (patientConsentHistory) {
			var cellData = $("<tr></tr>");
			var count = 1;
			var indexCounter = 1;
			var timestamps = [];
			for ( var timestamp in patientConsentHistory) {
				timestamps[timestamps.length] = parseInt(timestamp);
			}

			timestamps.sort(function(a, b) {
				return b - a;
			});

			$("#consentHistoryTable")
					.append(
							"<thead><tr><td width='25%'><b></b></td><td width='25%'><b></b></td><td width='25%'><b></b></td><td width='25%'></td></tr></thead>");

			for ( var index in timestamps) {
				var timestamp = timestamps[index];
				cellData
						.append($("<td patientId='" + patientId
						+ "' timestamp='" + timestamp + "' class='pastConsent'>"
								+ (indexCounter) + ") <a>" + timeConverter(timestamp) + "</a></td>"));
				if (count % 4 == 0) {
					$("#consentHistoryTable").append(cellData);
					cellData = $("<tr></tr>");
				}
				count++;
				indexCounter++;
			}
			if (timestamps.length > 0) {
				// If we remove the while loop, we get an error in case of missing cells
				while (count % 5 != 0) {
					count++;
					cellData.append($("<td></td>"));
				}
				$("#consentHistoryTable").append(cellData);
			}
			// 			$("#consentHistoryTable").dataTable({
			// 				"ordering" : false
			// 			});
		}
		$(".pastConsent").click(function() {
			var patientId = $(this).attr("patientId");
			var timestamp = $(this).attr("timestamp");
			var patientConsent = patientProfiles[patientId].consentHistory[timestamp];
			var patientUserProfile = patientProfiles[patientId].userProfile;
			var popup = $("#consent-modal-lg");
			$("#consentModalBody").html($("#formConsent").html());
			defineConsentHistorySignature(popup, patientConsent)
			for ( var param in patientConsent) {
				$("#" + param, popup).val(patientConsent[param]);
			}
			$("#consentModalBody :input", popup).attr("disabled", "disabled");
			$("#btnSubmitConsent", popup).addClass("hide");
			$("#consent_patient_name", popup).val(patientUserProfile.name);
			popup.modal();
		});
		
		function defineConsentHistorySignature($div, patientConsent){
			$(".signature", $div).empty()
			var $sigdivPatient = $(".signature", $div)
			$sigdivPatient.jSignature({
				'height' : 70,
				'width' : 300
			})
			
			$(".clear_signature", $div).addClass("hide")
			$(".signature", $div).jSignature("disable");
			
			var signatureDivs = $(".signature", $div);
			for (var index = 0; index < signatureDivs.length; index++){
				try {
					var sigDiv = signatureDivs[index];
					var $sigDiv = $(sigDiv);
					sigDivValue = patientConsent[$sigDiv.attr("attached_input")]
					$sigDiv.jSignature("setData", sigDivValue);
					$sigDiv.jSignature("disable");					
				} catch (err){
					/*Ignore*/
				}
			}
		}

		var patientMedicalHistoryLogs = patient.medicalHistoryLogs;
		$("#medicalHistoryLogTable").empty();
		if (patientMedicalHistoryLogs) {
			var cellData = $("<tr></tr>");
			var count = 1;
			var indexCounter = 1;
			var timestamps = [];
			for ( var timestamp in patientMedicalHistoryLogs) {
				timestamps[timestamps.length] = parseInt(timestamp);
			}

			timestamps.sort(function(a, b) {
				return b - a;
			});

			$("#medicalHistoryLogTable")
					.append(
							"<thead><tr><td width='25%'><b></b></td><td width='25%'><b></b></td><td width='25%'><b></b></td><td width='25%'></td></tr></thead>");
			for ( var index in timestamps) {
				var timestamp = timestamps[index];
				cellData
						.append($("<td  width='25%' patientId='" + patientId
						+ "' timestamp='" + timestamp
						+ "' class='medicalHistoryLog'>"
								+ (indexCounter) + ") <a>" + timeConverter(timestamp) + "</a></td>"));
				if (count % 4 == 0) {
					$("#medicalHistoryLogTable").append(cellData);
					cellData = $("<tr></tr>");
				}
				count++;
				indexCounter++;
			}
			if (timestamps.length > 0) {
				// If we remove the while loop, we get an error in case of missing cells
				while (count % 5 != 0) {
					count++;
					cellData.append($("<td></td>"));
				}
				$("#medicalHistoryLogTable").append(cellData);
			}
			// 			$("#medicalHistoryLogTable").dataTable({
			// 				"ordering" : false
			// 			});
		}

		$(".medicalHistoryLog").click(function() {
			var patientId = $(this).attr("patientId");
			var timestamp = $(this).attr("timestamp");
			var patientHistory = patientProfiles[patientId].medicalHistoryLogs[timestamp];
			var popup = $("#medical-modal-lg");
			$("#medicalModalBody").html($("#tabMEDICALHISTORY").html());
			setPatientMedicalHistoryLog(patientHistory, popup);

			$("#medicalModalBody :input").attr("disabled", "disabled");
			$('#btnDoneMedical', popup).addClass("hide");
			$('#btnAcceptMedical', popup).addClass("hide");
			$('#btnClearPatientSignature', popup).addClass("hide");
			$('#btnClearDentistSignature', popup).addClass("hide");
			popup.modal();

			try {
				if (patientHistory.patientSignature) {
					$("#patientSignature", popup).attr("id", "patientSignatureLog")
					$("#patientSignatureLog", popup).empty();
					$("#patientSignatureLog", popup).jSignature({
						'height' : 70,
						'width' : 300
					});
					$("#patientSignatureLog", popup).jSignature("setData", patientHistory.patientSignature);
				} else {
					$("#patientSignatureLog", popup).jSignature("reset")
				}
				if (patientHistory.dentistSignature) {
					$("#dentistSignature", popup).attr("id", "dentistSignatureLog")
					$("#dentistSignatureLog", popup).empty();
					$("#dentistSignatureLog", popup).jSignature({
						'height' : 70,
						'width' : 300
					});
					$("#dentistSignatureLog", popup).jSignature("setData", patientHistory.dentistSignature);
				} else {
					$("#dentistSignatureLog", popup).jSignature("reset")
				}
				$("#patientSignatureLog", popup).jSignature("disable");
				$("#dentistSignatureLog", popup).jSignature("disable");
			} catch (err) {
				//ignore
			}
		});
	}
</script>
	</div>
	
	
	
	
</div>
