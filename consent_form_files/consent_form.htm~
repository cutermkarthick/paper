<div class="modal fade" id="attachmentModal" tabindex="-1" role="dialog" aria-labelledby="attachmentModalTitle" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
				<h4 class="modal-title" id="attachmentModalTitle">Upload Attachment</h4>
			</div>
			<div class="modal-body">
				<p>Upload an attachment.</p>
				<form id="attachmentForm" method="post" enctype="multipart/form-data" action="https://www.paperlessdentists.com/portal/message/uploadAttachment">
					<div id="uploadBox" class="well well-sm show" onclick="$(&#39;#photoimgFront&#39;).click();">
						<p class="panel-header">Click to Upload</p>
					</div>
					<div class="hidden" id="progressbar-div">
						<div class="progress">
							<div class="progress progress-striped">
								<div class="progress-bar progress-bar-info" id="progress-bar-front" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">0% Complete</div>
							</div>
						</div>
					</div>
					<input type="file" class="hide" name="photoimgFront" id="photoimgFront" onchange="$(this.form).submit();">
				</form>
				<script type="text/javascript">
	$(document).ready(function() {
		
			
		$('#attachmentModal').on('show.bs.modal', function (e) {
			showFileUploadProgressBar("uploadBox", "progressbar-div",false);
		});
		
		$("#btnDeleteAttachment").click(function(){
			$("#attachment").val("");
			$("#attachmentFilename").text("");
			$("#attachmentDetailsDiv").addClass("hide")
		})
	
		$('#attachmentForm').ajaxForm( {
		    dataType : 'json',
		    beforeSend: function() {
				showFileUploadProgressBar("uploadBox", "progressbar-div",true);
		    },
		    uploadProgress: function ( event, position, total, percentComplete ) {
		        if (percentComplete == 100) {
					$('#progress-bar').css('width', percentComplete+'%').attr('aria-valuenow', percentComplete);
					$('#progress-bar').html('Processing ...');
		        } else {
					$('#progress-bar').css('width', percentComplete+'%').attr('aria-valuenow', percentComplete);
					$('#progress-bar').html(percentComplete);
		        }
		    },
		    success : function ( response ) {
				$('#progress-bar').css('width','100%').html('100%');
				responseJson = response
				if (responseJson.filename){
					$("#attachment").val(responseJson.filename);
					$("#attachmentFilename").text(responseJson.original_filename);
					$("#attachmentDetailsDiv").removeClass("hide")
				}
				showFileUploadProgressBar("uploadBox", "progressbar-div",false);
				$("#uploadBox").html("<p class=\"succcess\">" + responseJson.message +"</p>");
		    },
			complete: function(response) {
				showFileUploadProgressBar("uploadBox", "progressbar-div",false);
				responseJson = response.responseJSON
				$("#uploadBox").html("<p class=\"succcess\">" + responseJson.message +"</p>");
			},
			error: function(response) {
				responseJson = JSON.parse(response.responseText)
				showFileUploadProgressBar("uploadBox", "progressbar-div",false);
				$("#uploadBox").html("<p class=\"danger\">" + responseJson.message +"</p>");
			}
		});
	
	}); //end of $(document).ready(function() {
 </script>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
<script>
	var imagePath = 'http://162.209.124.93:8888/Images'
	unread_count = 0;
	function defineMessages() {

		$("#inbox_link").click(function() {
			 //showInboxOutboxMessagesForDentist('dentistInboxTable', '/portal/dentists/message/inbox', 'inbox');
			 showDentistInboxInDataTable();
		});

		$("#outbox_link").click(function() {
			//showInboxOutboxMessagesForDentist('dentistOutboxTable', '/portal/dentists/message/outbox', 'outbox');
			showDentistOutboxInDataTable();
		});
		
		defineDeleteMessage();
		defineReplyMessageDentist();
	}
	
	
	var dataTableInstanceInbox = null;
	var dataTableInstanceOutbox = null;
	
	function showDentistInboxInDataTable(){
		var msgTable = $("#dentistInboxTable");
		var ajaxUrl = '/portal/dentists/message/inbox';
		if(! $.fn.dataTable.isDataTable('#dentistInboxTable')){
			dataTableInstanceInbox = msgTable.DataTable({
				"order": [[ 2, "desc" ]],
				"retrieve": true,
				"deferRender": true,
				"ajax": {"url": ajaxUrl, "type":"GET"},
				"columns": [
				            { "data": "message_id", "visible": false, "targets" : [0]},
				            { "data": "status", "visible": false, "targets" : [1]},
				            { "data": "date", "visible": false, "targets" : [2]},
				            { "data": "user_name","visible": true},
				            { "data": "subject",  "visible": true},
				            { "data": "message" , "visible": true},
				            { "data": "duration", "visible": true}
				        ],
		        "createdRow" : function(row, data, index){
						 
					 	
					 	var duration = parseInt(data.duration);
						var durationText = data.date;
						if (Math.abs(duration) < 1000 * 60 * 60 * 24) {
							durationText = moment.duration(duration).humanize(true);
						}
						$('td', row).eq(3).text(durationText);
						
					 	var messageStatusClass = (((data.status == "opened" || data.status == "OPENED")) ? "unhighlight"
								: "highlight");
						for(var j=0;j<4;j++){
						 	
					 		$('td', row).eq(j).addClass(messageStatusClass);
							
							$('td', row).eq(j).addClass("ellipsis");
					 	}
				}
			});
			reloadUnreadBadge();
		} else {
			
			dataTableInstanceInbox.ajax.reload();
		}
		
	}
	
	function reloadUnreadBadge(){
		try{
			unread_count = 0
			table_data = $("#dentistInboxTable").DataTable().ajax.json().data;
			for (index in table_data){
				data = table_data[index];
				if (data.status != "opened" && data.status != "OPENED"){
					unread_count++;
				}
			}
			$("#inbox_badge").text(unread_count);
			$("#messages_badge").text(unread_count);
		} catch(err){
			/* Ignore */
		}
	}
	
	
	function showDentistOutboxInDataTable(){
		var msgTable = $("#dentistOutboxTable");
		var ajaxUrl = '/portal/dentists/message/outbox';
		if(! $.fn.dataTable.isDataTable('#dentistOutboxTable')){
			dataTableInstanceOutbox = msgTable.DataTable({
				"aaSorting": [[ 2, "desc" ]],
				"retrieve": true,
				"deferRender": true,
				"ajax": {"url": ajaxUrl, "type":"GET"},
				"columns": [
				            { "data": "message_id", "visible": false, "targets" : [0]},
				            { "data": "status", "visible": false, "targets" : [1]},
				            { "data": "date", "visible": false, "targets" : [2]},
				            { "data": "user_name","visible": true},
				            { "data": "subject",  "visible": true},
				            { "data": "message" , "visible": true},
				            { "data": "duration", "visible": true}
				        ],
		        "createdRow" : function(row, data, index){
						 
					 	
					 	var duration = parseInt(data.duration);
						var durationText = data.date;
						if (Math.abs(duration) < 1000 * 60 * 60 * 24) {
							durationText = moment.duration(duration).humanize(true);
						}
						$('td', row).eq(3).text(durationText);
						
					 	var messageStatusClass = "unhighlight";
						for(var j=0;j<4;j++){
						 	
					 		$('td', row).eq(j).addClass(messageStatusClass);
							
							$('td', row).eq(j).addClass("ellipsis");
					 	}
				}   
			});
		} else {
			
			dataTableInstanceOutbox.ajax.reload();
			//messagesDataTableInstance.ajax.url( ajaxUrl ).load();
		}
	}
	
	
	$(document).ready(function() {
		$("#viewDentistMessageModal").offcanvas({toggle: false, placement:'right', disableScrolling:false})
		
		$('#dentistInboxTable tbody').on('click', 'tr', function (e) {
			var messagesDataTable = $('#dentistInboxTable').DataTable();
			var messageData = messagesDataTable.row(this).data();
			
			for(var j=0;j<4;j++){
				$('td', this).eq(j).removeClass('highlight');
				$('td', this).eq(j).addClass('unhighlight');
			}		
			
			var msgDataJson = JSON.stringify(messageData);
			var message = messageData;
			$("#viewDentistMessageModal").offcanvas('show');
			$("#viewMessageHolder").val(msgDataJson);
			$("#label_dentist").text("To Dentist");
			$("#label_patient").text("From Patient");
			$("#viewDentistName").text(messageData.dentist_name);
			$("#viewPatientName").text(messageData.user_name);
			$("#viewSubject").text(messageData.subject);
			$("#viewMessageText").html(messageData.message);
			if (messageData.attachment){
				$("#viewAttachmentDetailsDiv").removeClass("hide");
				$("#viewAttachment").text(messageData.attachment);
				$("#viewAttachment").attr("href", imagePath + "/" + messageData.attachment);
			} else {
				$("#viewAttachmentDetailsDiv").addClass("hide");
			}	 
			
			
			var data = { messageId : messageData.message_id};
			if (message.status != "OPENED"){
				unread_count = Math.max(unread_count - 1, 0)
				$("#inbox_badge").text(unread_count);
				$("#messages_badge").text(unread_count);
				$.ajax({
					url : '/portal/message/read',
					data : data,
					type : "POST"
				});
			}
			 
			$("#btnDeleteMessage").attr("messageId", messageData.message_id);
			$("#btnDeleteMessage").attr("msgBoxType", 'inbox');
			$("#btnReplyMessage").attr("messageId", messageData.message_id);
			e.preventDefault();
			e.stopPropagation();
		}); 
		
		
		$('#dentistOutboxTable tbody').on('click', 'tr', function (e) {
			var messagesDataTable = $('#dentistOutboxTable').DataTable();
			var messageData = messagesDataTable.row(this).data();
			var message = messageData;
			
			for(var j=0;j<4;j++){
				$('td', this).eq(j).removeClass('highlight');
				$('td', this).eq(j).addClass('unhighlight');
			}		
			var msgDataJson = JSON.stringify(messageData);
			$("#viewDentistMessageModal").offcanvas('show');
			$("#viewMessageHolder").val(msgDataJson);
			$("#label_dentist").text("From Dentist");
			$("#label_patient").text("To Patient");
			$("#viewDentistName").text(messageData.dentist_name);
			$("#viewPatientName").text(messageData.user_name);
			$("#viewSubject").text(messageData.subject);
			$("#viewMessageText").html(messageData.message);
			if (messageData.attachment){
				$("#viewAttachmentDetailsDiv").removeClass("hide");
				$("#viewAttachment").text(messageData.attachment);
				$("#viewAttachment").attr("href", imagePath + "/" + messageData.attachment);
			} else {
				$("#viewAttachmentDetailsDiv").addClass("hide");
			}	 
			$("#btnDeleteMessage").attr("messageId", messageData.message_id);
			$("#btnDeleteMessage").attr("msgBoxType", 'outbox');
			$("#btnReplyMessage").attr("messageId", messageData.message_id);
			e.preventDefault();
			e.stopPropagation();
		}); 
	});
	

	function defineDeleteMessage() {
		$("#btnDeleteMessage").click(function() {
			var messageId = $(this).attr("messageId");
			var msgBoxType = $(this).attr("msgBoxType");
			var data = {
				messageId : messageId
			};
			$.ajax({
				url : '/portal/message/delete',
				data : data ,
				type : "POST"
			}).complete(function(msg) {
				$("#viewDentistMessageModal").offcanvas("hide");
				if(msgBoxType == 'inbox'){
					dataTableInstanceInbox.ajax.reload();
				} else {
					dataTableInstanceOutbox.ajax.reload();
				}
			});
		});
	}

	function defineReplyMessageDentist() {
		$("#btnReplyMessage").click(function() {
			history.pushState(null, null, "#compose");
			//var messageId = $(this).attr("messageId");
			var message = JSON.parse($("#viewMessageHolder").val());
			$("#messagePatientName").val(message.user_id).trigger("change");
			$("#viewDentistMessageModal").offcanvas("hide");
			$('#collapseTwo a:first').tab('show')
			$("#subject").val("RE : " + message.subject);
			$("#message").val("\n\n-------------------- \n" + message.message);
		});
	}

	function setComposePatients(allPatients, $select) {
		for ( var index in allPatients) {
			var patient = allPatients[index];
			$select.append($("<option value='" + patient.id + "'>" + patient.name + " </option>"));
		}
	}

	function defineMessagesTabValidator() {

	var validatorMessagesTab = null;
		validatorMessagesTab = $("#compose_form").bind(
				"invalid-form.validate",
				function() {
					$("#error-summary-top-messages").html(
							"Your form contains " + validatorMessagesTab.numberOfInvalids()
									+ " errors, see details below.");
					$("#error-summary-bottom-messages").html(
							"Your form contains " + validatorMessagesTab.numberOfInvalids()
									+ " errors, see details above.");
				}).validate({
			errorContainer : $("#error-summary-top-messages, #error-summary-bottom-messages"),
			rules : {
				messagePatientName : {
					required : true
				},
				subject : {
					maxlength : 128,
					required : true
				},
				message : {
					maxlength : 2048
				}
			},
			highlight : function(element) {
				$(element).closest('.form-control').addClass('has-error');
			},

			unhighlight : function(element) {
				$(element).closest('.form-control').removeClass('has-error');
			},
			errorElement : 'span',
			errorClass : 'alert-block',
			errorPlacement : function(error, element) {
				if (element.parent('.form-control').length) {
					error.insertAfter(element.parent());
				} else {
					error.insertAfter(element);
				}
			}
		});
	}

	function setMessageNotificationMessage(message) {
		var notificationDiv = $(".message_notification")
		notificationDiv.removeClass("hide");
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	$(document).ready(function() {
		 $('#compose_form input[type=checkbox]').attr('checked',false);
		 $('#btnDoneCompose').on('click', function() {
			var isValidMessageTab = $('#compose_form').valid();
			if (isValidMessageTab) {
				$('#compose_form').submit();
			}
		});

		// bind 'Compose Form' and provide a simple callback function 
		$('#compose_form').ajaxForm({
			success : function(responseText) {
				$("#subject").val("")
				$("#message").val("")
				$('#collapseTwo a:eq(1)').tab('show')
				$("#attachment").val("");
				$("#attachmentFilename").text("");
				$("#attachmentDetailsDiv").addClass("hide")
				if (responseText.REASON) {
					setMessageNotificationMessage(responseText.REASON);
				} else {
					setMessageNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setMessageNotificationMessage("Error occurred. Please try again later.");
			}
		});
		defineMessagesTabValidator();
		defineMessages();
		$("#outbox_link").click();
		$("#compose_link").click();
		$("#inbox_link").click();
		//$("#dentistInboxTable").width("100%")
		showRemainingChars('message', 'message_feedback', 2048);
		
		$('#dentistInboxTable').on('xhr.dt', function ( e, settings, json ) {
			reloadUnreadBadge();
		});
		
		$('#dentistInboxTable').on('draw.dt', function ( e, settings, json ) {
			reloadUnreadBadge();
		});
	})
</script>
		</div>
		<div class="col col-centered col-xs-8 col-md-9 col-lg-9 col-top hide" id="appointments_view">
			





