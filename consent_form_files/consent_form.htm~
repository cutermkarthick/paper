<script type="text/javascript">
	function setPatientEmploymentInfo(patientId){
		var patient = patientProfiles[patientId];
		var patientEmploymentProfile = patient.employmentProfile;
		if (patientEmploymentProfile) {
			$("#employmentStatus").html(patientEmploymentProfile.employmentStatus);
			$("#employmentOccupation").html(
					patientEmploymentProfile.employmentOccupation);
			$("#employmentCompany").html(patientEmploymentProfile.employmentCompany);
			$("#employmentAddress1").html(
					patientEmploymentProfile.employmentAddress1);
			$("#employmentAddress2").html(
					patientEmploymentProfile.employmentAddress2);
			$("#employmentCity").html(patientEmploymentProfile.employmentCity);
			$("#employmentState").html(patientEmploymentProfile.employmentState);
			$("#employmentZip").html(patientEmploymentProfile.employmentZip);
		} else {
			$("#employmentStatus").html("");
			$("#employmentOccupation").html("");
			$("#employmentCompany").html("");
			$("#employmentAddress1").html("");
			$("#employmentAddress2").html("");
			$("#employmentCity").html("");
			$("#employmentState").html("");
			$("#employmentZip").html("");
		}
	}
</script>
