<script type="text/javascript">
	function setPatientPersonalInfo(patientId) {
		var patient = patientProfiles[patientId];
		var patientUserProfile = patient.userProfile;
		$("#patientImage").attr("src", imagePath + "/" + patientId + ".img");
		$("#email").html(patientUserProfile.email);
		$("#firstName").html(patientUserProfile.firstName);
		$("#middleName").html(patientUserProfile.middleName);
		$("#lastName").html(patientUserProfile.lastName);
		$("#address1").html(patientUserProfile.address1);
		$("#address2").html(patientUserProfile.address2);
		$("#city").html(patientUserProfile.city);
		$("#state").html(patientUserProfile.state);
		$("#zip").html(patientUserProfile.zip);
		$("#cellPhones").html(patientUserProfile.cellPhones);
		$("#workPhones").html(patientUserProfile.workPhones);
		$("#homePhones").html(patientUserProfile.homePhones);
		$("#gender").html(patientUserProfile.gender);
		$("#dob").html(patientUserProfile.dob);
		$("#contactmethod").html(patientUserProfile.contactmethod);
		$("#maritalStatus").html(patientUserProfile.maritalStatus);
		$("#spouseName").html(patientUserProfile.spouseName);
		$("#improveSmile").html((patientUserProfile.improveSmile) ? "Yes" : "No");
		$("#improveSmileText").html(patientUserProfile.improveSmileText);
		$("#smileRating").html(patientUserProfile.smileRating);
		$("#happyWithSmile").html((patientUserProfile.happyWithSmile) ? "Yes" : "No");
	}
</script>

