	<div class="tab-pane active" id="new_appointment">
		<div class="panel panel-primary">
			<div class="panel-heading text-center">
				<h3 class="panel-title">Book an Appointment</h3>
			</div>
			<div class="panel-body">
				<form role="form" name="new_appointment_form" id="new_appointment_form" autocomplete="off" class="form-horizontal" action="https://www.paperlessdentists.com/portal/dentists/appointment/new" method="POST" novalidate="novalidate">
					<input name="dentistId" id="dentistId" value="2" type="hidden"> <input name="start_time" id="start_time" type="hidden"> <input name="end_time" id="end_time" type="hidden"> <input name="timezone" id="timezone" type="hidden" value="America/Los_Angeles">
					
					<div class="form-group">
						<label for="patient" class="col-sm-3 control-label">Patient</label>
						<div class="col-sm-8">
