		var imagePath = 'http://162.209.124.93:8888/Images';
		var patient = patientProfiles[patientId];
		$("#quickViewUserId").val(patientId);
		$("#referUserId").val(patientId);
		$("#btnDetailedView").attr("patientId", patientId);

		var patientUserProfile = patient.userProfile;
		//Changes by Akash 9/11/2014
		//show patient name under signature area in the consent form
		$(".consent_patient_name").attr("value", patientUserProfile.firstName + " " + patientUserProfile.middleName + " " + patientUserProfile.lastName);
		//show selected patient name at the top of the body.
		$("#selected_patient_name").attr("value", patientUserProfile.firstName + " " + patientUserProfile.middleName + " " + patientUserProfile.lastName);
		$("#showPatientName").show();
		$(".hidePatientName").click(function(){
			$("#showPatientName").hide();
		});
		//end of changes by Akash 9/11/2014
		
		$("#quickViewName").html(
				patientUserProfile.firstName + " " + patientUserProfile.middleName + " " + patientUserProfile.lastName);
		$("#quickViewAddress1").html(patientUserProfile.address1);
		$("#quickViewAddress2").html(patientUserProfile.address2);
		$("#quickViewAddress3").html(patientUserProfile.city + " " + patientUserProfile.zip);
		$("#quickViewCellPhone").html(patientUserProfile.cellPhones);
		$("#quickViewHomePhone").html(patientUserProfile.homePhones);
		//TODO: KECHIT SHOULD WE BE SHOWING WORK PHONE ALSO?
		$("#quickViewWorkPhone").html(patientUserProfile.workPhones);
		$("#quickViewDob").html(patientUserProfile.dob);
		$("#quickViewEmail").html(patientUserProfile.email);
		//Show the patient image
		$("#quickViewPatientImage").attr("src", imagePath + "/" + patientId + ".img");
		//Employment info
		var patientEmploymentProfile = patient.employmentProfile;
		if (patientEmploymentProfile) {
			$("#quickViewEmployer").html(
					patientEmploymentProfile.employmentCompany ? patientEmploymentProfile.employmentCompany : "");
		} else {
			$("#quickViewEmployer").html("");
		}
		//Emergency Contact info
		var patientEmergencyProfile = patient.emergencyProfile;

		if (patientEmergencyProfile) {
			$("#quickViewEmergency").html(
					patientEmergencyProfile.emergencyFirstName + " " + patientEmergencyProfile.emergencyMiddleName
							+ " " + patientEmergencyProfile.emergencyLastName);
		} else {
			$("#quickViewEmergency").html("");
		}

		//Referred To and By
		var referalProfile = patient.referal;
		if (referalProfile) {
			$("#quickViewReferredTo").html(referalProfile.refered_dentist_name);
			$("#quickViewReferredBy").html(referalProfile.dentist_name);
		} else {
			$("#quickViewReferredTo").html("");
			$("#quickViewReferredBy").html("");
		}
		//Primary Insurance info
		var patientInsuranceProfile = patient.insuranceProfile;

		if (patientInsuranceProfile) {
			$("#quickViewInsuranceCompany").html(patientInsuranceProfile.company1);
			$("#quickViewGroupId").html(patientInsuranceProfile.group_id1);
			$("#quickViewMemberId").html(patientInsuranceProfile.member_id1);
		} else {
			$("#quickViewInsuranceCompany").html("");
			$("#quickViewGroupId").html("");
			$("#quickViewMemberId").html("");
		}

		var patientMedicalProfile = patient.medicalprofile;
		var $alertButton = $("#btnMedicalAlert");
		if (patientMedicalProfile && hasMedicalAlertCondition(patientMedicalProfile)) {
			$alertButton.removeClass("hide");
			$alertButton.addClass("btn-danger");
		} else {
			$alertButton.addClass("hide");
			$alertButton.removeClass("btn-danger");
			$alertButton.addClass("btn-success");
		}
		//patient survey
		var patientSurvey = patient.survey;
		//var psurvey = JSON.stringify(patientSurvey.data);
		$("#quickSurveyResults").val(patientSurvey ? patientSurvey.data : "");
		// Set the patient id in the messages & appointments
		$("#messagePatientName").val(patientId).trigger("change");
		$("#appointmentPatientName").val(patientId).trigger("change");
	}
</script>
