	var referPatients = JSON.parse('[]');
	var newAppointmentRequests = JSON.parse('[]');
	

	function refreshPatient(patientId){
		$.ajax({
			url : applicationContext + '/dentists/refreshPatient/' + patientId,
			data : {},
			type : "GET"
		}).success(function(response) {
			if (response.SUCCESS) {
				dataJson = response.DATA;
				patientProfiles[patientId] = dataJson;
				setPatientUserHistory(patientId);
			}
		});
	}
	
	function hideAllDivs() {
		$("#dentist_dashboard").addClass("hide");
		$("#patient_detailed_view").addClass("hide");
		$("#patient_brief_view").addClass("hide");
		$("#messages_view").addClass("hide");
		$("#appointments_view").addClass("hide");
	}

	function defineNavigation() {
		$("#inbox_link").click(function() {
			hideAllDivs();
			$("#messages_view").removeClass("hide");
		});
		$("#outbox_link").click(function() {
			hideAllDivs();
			$("#messages_view").removeClass("hide");
		});
		$("#compose_link").click(function() {
			hideAllDivs();
			$("#messages_view").removeClass("hide");
		});
		$("#new_patients_link").click(function() {
			hideAllDivs();
			$("#dentist_dashboard").removeClass("hide");
		});
		$("#all_patients_link").click(function() {
			hideAllDivs();
			$("#dentist_dashboard").removeClass("hide");
		});
		$("#referred_patients_link").click(function() {
			hideAllDivs();
			$("#dentist_dashboard").removeClass("hide");
		});
		$("#appointment_request_link").click(function() {
			hideAllDivs();
			$("#appointments_view").removeClass("hide");
		});
		$("#my_calendar_link").click(function() {
			hideAllDivs();
			$("#appointments_view").removeClass("hide");
		});
		$("#new_appointment_link").click(function() {
			hideAllDivs();
			$("#appointments_view").removeClass("hide");
		});

	}
	
	function setPatientDetailedView(patientId) {
		$("#downloadUserId").val(patientId);
		setPatientPersonalInfo(patientId);
		setPatientEmergencyInfo(patientId);
		setPatientEmploymentInfo(patientId);
		setPatientInsuranceInfo(patientId);
		setPatientDentalInfo(patientId);
		setPatientMedicalInfo(patientId);
		setPatientConsentInfo(patientId);
		setPatientUserHistory(patientId);
		
		
		//setPatientTreatmentNotes(patientId);
		// Set the patient id in the messages & appointments
		$("#messagePatientName").val(patientId).trigger("change");
		$("#appointmentPatientName").val(patientId).trigger("change");
	}
	
	/*
	 FUNCTION TO SET THE 'BOOTSTRAP v3 DATATABLES.NET' PLUGIN BASED TABLE
	 Set the data in client side javascript array and then render the bootstrap3 datatable. 
	 Refer http://www.datatables.net/examples/data_sources/js_array.html 
	 START: This is for rendering the ALL PATIENTS and NEW PATIENTS datatable
	*/
	function setPatientsDataTable(patientData, patientsTableId){
		var dataSet = new Array();
		if(patientData){
			var indexCounter = 1;
			for (var index in patientData){
				var patient = patientData[index];
				var patientId = patient.id;
				var displayName = patient.name;
				var medicalStatus = patient.medicalStatus;
				var insuranceStatus = patient.insuranceStatus;
				
				var dataSetElem = [patientId, indexCounter, displayName, medicalStatus, insuranceStatus];
				dataSet.push(dataSetElem);
				indexCounter++;
			}
		}
		
		patientsTableId.dataTable({
			"data": dataSet,
			"deferRender" : true,
		 	"columnDefs":[
		 	              	{"visible" : false, "targets" : [0]}            
			 			 ]
		});
	}
	
	
	
	
	
	
				
		           
		
	

	
	
	$('#allPatientsTable tbody').on('click', 'tr', function () {
		var rowNo = $('td', this).eq(0).text();
		var patientId = $('#allPatientsTable').dataTable().api().data()[parseInt(rowNo)-1][0]
		console.log('You clicked on All Patients table - patientId: '+patientId+'\'s row' );
		hideAllDivs();
		$("#patient_brief_view").removeClass("hide");
		setPatientBriefView(patientId);
		$("#tabDentistTreatmentNotesForm input[name='userId']").val(patientId);
		history.pushState(null, null, "#briefView");
	});
	
	$('#newPatientsTable tbody').on('click', 'tr', function () {
		var rowNo = $('td', this).eq(0).text();
		var patientId = $('#newPatientsTable').dataTable().api().data()[parseInt(rowNo)-1][0]
		console.log('You clicked on New Patients table - patientId: '+patientId+'\'s row' );
		hideAllDivs();
		$("#patient_brief_view").removeClass("hide");
		setPatientBriefView(patientId);
		$("#tabDentistTreatmentNotesForm input[name='userId']").val(patientId);
		history.pushState(null, null, "#briefView");
	});
	
	
	
	



	function hideAllCollapses(show_href) {
		//$("#collapseOne").collapse("hide")
		//$("#collapseTwo").collapse("hide")
		//$("#collapseThree").collapse("hide")
	}

	function showHideBasedOnHash(hash) {
		if (!hash || hash == "#tabNewPatients" || hash == "#tabAllPatients" ) {
			hideAllCollapses("#collapseOne")
			hideAllDivs();
			$("#dentist_dashboard").removeClass("hide");
			//$("#collapseOne").collapse("show")
		} else if (hash == "#compose" || hash == "#inbox" || hash == "#outbox") {
			hideAllCollapses("#collapseTwo")
			hideAllDivs();
			$("#messages_view").removeClass("hide");
			//$("#collapseTwo").collapse("show")
		} else if (hash=="#appointment_request" || hash == "#my_calendar" || hash == "#new_appointment") {
			hideAllCollapses("#collapseThree")
			hideAllDivs();
			$("#appointments_view").removeClass("hide");
			//$("#collapseThree").collapse("show")
		} else if (hash == "#briefView") {
			hideAllCollapses()
			hideAllDivs();
			$("#patient_brief_view").removeClass("hide");
		} else {
			hideAllCollapses()
			hideAllDivs();
			$("#patient_detailed_view").removeClass("hide");
		}
	}
	
	function getStartHash(hash){
		if (hash == "#tabNewPatients" || hash == "#tabAllPatients"  || hash == "#compose" || hash == "#inbox" || hash == "#outbox" || hash == "#appointment_request" || hash == "#my_calendar" || hash == "#new_appointment") {
			return hash;			
		}
		return "#tabNewPatients"
	}
	
	function showTab(hash){
		var activeTab = $('[href=' + hash + ']');
		showHideBasedOnHash(hash);
		if (activeTab.length) {
			activeTab.tab('show');
		} else {
			$('#collapseOne a:first').tab('show');
		}
	}

	
	$(document).ready(function() {
		
		if (newPatients != null){
			$("#new_patients_badge").text(newPatients.length);
			$("#patients_badge").text(newPatients.length);
		} else {
			$("#new_patients_badge").text(0);
			$("#patients_badge").text(0);
		}
		setPatientsDataTable(newPatients, $("#newPatientsTable"));
		setPatientsDataTable(allPatients, $("#allPatientsTable"));
		
		
			
		setComposePatients(allPatients, $("#messagePatientName"));
		setComposePatients(allPatients, $("#appointmentPatientName"));
		
		//start new appointment
		if (newAppointmentRequests != null){
			$("#appointment_badge").text(newAppointmentRequests.length);
			$("#appointment_request_badge").text(newAppointmentRequests.length);
		} else {
			$("#appointment_badge").text(0);
			$("#appointment_request_badge").text(0);
		}
		//setNewAppointmentRequestTable(newAppointmentRequests, $("#newAppointmentRequestTable"));
		showNewAppointmentsDataTable();
		//end new appointment
		defineNavigation();
		
		
		
		$('a[data-toggle="tab"]').on('click', function(e) {
			history.pushState(null, null, $(this).attr('href'));
		});
		
		window.addEventListener("popstate", function(e) {
			showTab(location.hash);
		});
		
		var hash = window.location.hash;
		showTab(getStartHash(hash));
	});
</script>

