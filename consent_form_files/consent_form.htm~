<div class="modal fade" id="insuranceCardUploadModal" tabindex="-1" role="dialog" aria-labelledby="insuranceCardUploadModal" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
				<h4 class="modal-title" id="insuranceCardUploadModalLabel"></h4>
			</div>
			<div class="modal-body">
				<form id="dentistViewInsuranceCardUploadModalForm" method="post" enctype="multipart/form-data" action="https://www.paperlessdentists.com/portal/common/uploadinsurancecard">
					<input type="hidden" name="cardSide" id="cardSide"> <input type="hidden" name="insuranceOrder" id="insuranceOrder"> <input type="hidden" name="userId" class="insuranceUserId" value="23">
					<div id="uploadBoxFront-insurance" class="well well-sm show" onclick="$(&#39;#cardPhoto&#39;).click();">
						<p class="panel-header">Click to Upload</p>
					</div>
					<div class="hidden" id="progressbar-div-front-insurance">
						<div class="progress">
							<div class="progress progress-striped">
								<div class="progress-bar progress-bar-info" id="progress-bar-front-insurance" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">0% Complete</div>
							</div>
						</div>
					</div>
					<input type="file" class="hide" name="cardPhoto" id="cardPhoto" onchange="$(this.form).submit();">
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>

<script type="text/javascript">
	function defineInsuranceTabUploadCardPhoto() {

	$('#dentistViewInsuranceCardUploadModalForm')
				.ajaxForm(
						{
							dataType : 'json',
							beforeSend : function() {
								showFileUploadProgressBar("uploadBoxFront-insurance",
										"progressbar-div-front-insurance", true);
							},
							uploadProgress : function(event, position, total, percentComplete) {
								if (percentComplete == 100) {
									$('#progress-bar-front-insurance').css('width', percentComplete + '%').attr(
											'aria-valuenow', percentComplete);
									$('#progress-bar-front-insurance').html('Processing ...');
								} else {
									$('#progress-bar-front').css('width', percentComplete + '%').attr('aria-valuenow',
											percentComplete);
									$('#progress-bar-front-insurance').html(percentComplete);
								}
							},
							success : function(response) {
								$('#progress-bar-front-insurance').css('width', '100%').html('100%');
								responseJson = response
								if (responseJson.filename) {
									cardSide = responseJson.cardSide;
									insuranceOrder = responseJson.insuranceOrder;

	$("#" + insuranceOrder + "_" + cardSide).attr("src",
											responseJson.filename + '?ts=' + (new Date()).getTime())
								}
								showFileUploadProgressBar("uploadBoxFront-insurance",
										"progressbar-div-front-insurance", false);
								$("#uploadBoxFront-insurance").html(
										"<p class=\"succcess\">" + responseJson.message + "</p>");
							},
							complete : function(response) {
								showFileUploadProgressBar("uploadBoxFront-insurance",
										"progressbar-div-front-insurance", false);
								responseJson = response.responseJSON
								$("#uploadBoxFront-insurance").html(
										"<p class=\"succcess\">" + responseJson.message + "</p>");
							},
							error : function(response) {
								responseJson = JSON.parse(response.responseText)
								showFileUploadProgressBar("uploadBoxFront-insurance",
										"progressbar-div-front-insurance", false);
								$("#uploadBoxFront-insurance").html(
										"<p class=\"danger\">" + responseJson.message + "</p>");
							}
						});
	}

	function defineInsuranceTabInsuranceCardPhotoUploadUIBehavior() {

	$('#btnInsuranceCard-Primary-Front').on(
				"click",
				function() {
					var cardSide = "front";
					var insuranceOrder = "primary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>front</b> side of the insurance card.");
				});

	$('#btnInsuranceCard-Primary-Back').on(
				"click",
				function() {
					var cardSide = "back";
					var insuranceOrder = "primary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>back</b> side of the insurance card.");
				});

	$('#btnInsuranceCard-Secondary-Front').on(
				"click",
				function() {
					var cardSide = "front";
					var insuranceOrder = "secondary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>front</b> side of the insurance card.");
				});

	$('#btnInsuranceCard-Secondary-Back').on(
				"click",
				function() {
					var cardSide = "back";
					var insuranceOrder = "secondary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>back</b> side of the insurance card.");
				});

	$('#btnInsuranceCard-Tertiary-Front').on(
				"click",
				function() {
					var cardSide = "front";
					var insuranceOrder = "tertiary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>front</b> side of the insurance card.");
				});

	$('#btnInsuranceCard-Tertiary-Back').on(
				"click",
				function() {
					var cardSide = "back";
					var insuranceOrder = "tertiary";
					$(".modal-body #cardSide").val(cardSide);
					$(".modal-body #insuranceOrder").val(insuranceOrder);
					$(".modal-header #insuranceCardUploadModalLabel").html(
							"Upload a picture of the <b class='text-danger'>back</b> side of the insurance card.");
				});

	$('#insuranceCardUploadModal').on('show.bs.modal', function(e) {
			showFileUploadProgressBar("uploadBoxFront-insurance", "progressbar-div-front-insurance", false);
		});

	$("#insuranceCardUploadModal").on('hidden.bs.modal', function() {
			$(".modal-body #cardSide").val("");
			$(".modal-body #insuranceOrder").val("");
			$(".modal-header #insuranceCardUploadModalLabel").html("");
		});

	}
	
	function defineInsuranceInfoTabValidator(){
		
			var validatorInsuranceInfoTab= null;
			validatorInsuranceInfoTab = $("#dentistInsuranceInfoForm").bind("invalid-form.validate", function() {
				$("#error-summary-top-insurance").html("Your form contains " + validatorInsuranceInfoTab.numberOfInvalids() + " errors, see details below.");
				$("#error-summary-bottom-insurance").html("Your form contains " + validatorInsuranceInfoTab.numberOfInvalids() + " errors, see details above.");
			}).validate({
				errorContainer: $("#error-summary-top-insurance, #error-summary-bottom-insurance"),
				rules: {
					primaryInsuranceHolder:{ required: true },
					company1: {
						maxlength: 256
					},
					company2: {
						maxlength: 256
					},
					company3: {
						maxlength: 256
					},
					group_id1: {
						maxlength: 64
					},
					group_id2: {
						maxlength: 64
					},
					group_id3: {
						maxlength: 64
					},
					member_id1: {
						maxlength: 64
					},
					member_id2: {
						maxlength: 64
					},
					member_id3: {
						maxlength: 64
					},
					dentalInsuranceId : {
						maxlength : 32
					},
					planName : {
						maxlength : 128
					},
					primaryDob : {
						customdatevalidator : true,
						pastdatevalidator : true
					}
					
					
				},
				messages: {
					ssn1 : "SSN is required for your dental provider to process your insurance",
					ssn2 : "SSN is required for your dental provider to process your insurance",
					ssn3 : "SSN is required for your dental provider to process your insurance"
				},
				highlight: function(element){
					$(element).closest('.form-control').addClass('has-error');
				}, 
				
				unhighlight: function(element) {
			        $(element).closest('.form-control').removeClass('has-error');
			    },
			    errorElement: 'span',
			    errorClass: 'alert-block',
			    errorPlacement: function(error, element) {
			        if(element.parent('.form-control').length) {
			            error.insertAfter(element.parent());
			        } else {
			            error.insertAfter(element);
			        }
			    }
			});
			
	}


	function defineInsuranceTabSubmitHandler(){
		
		//http://jsfiddle.net/mapb_1990/hTPY7/9/ use this
		$('#btnSubmitTabInsuranceInfo').on('click', function() {
			var isValidInsuranceInfoTab= $('#dentistInsuranceInfoForm').valid();
			if(isValidInsuranceInfoTab) {
				//assemble the SSN
				
				$('#dentistInsuranceInfoForm').submit();
			}
		});	
		
		function setInsuranceNotificationMessage(message, type) {
			var notificationDiv = $(".insurance_notification")
			notificationDiv.removeClass("alert-danger")
			notificationDiv.removeClass("alert-success")
			notificationDiv.addClass("alert-" + type)
			notificationDiv.removeClass("hide");
			notificationDiv.text(message);
			setTimeout(function() {
				notificationDiv.addClass("hide");
			}, 15000);
		}

		$('#dentistInsuranceInfoForm').ajaxForm({
			success : function(responseText) {
				if (responseText.SUCCESS) {
					if (responseText.REASON) {
						setInsuranceNotificationMessage(responseText.REASON, "success");
					} else {
						setInsuranceNotificationMessage("The Insurance Information has been updated successfully.", "success");
					}
					var userId = $("#insuranceUserId").val()
					refreshPatient(userId);
				} else {
					setInsuranceNotificationMessage("Error occurred. Please try again later.", "danger");
				}
			},
			error : function(responseText) {
				setInsuranceNotificationMessage("Error occurred. Please try again later.", "danger");
			}
		});
	}

	function applyTimestamp(originalUrl) {
		var i = originalUrl.indexOf('?ts=');
		var newUrl = ((i != -1) ? originalUrl.substring(0, i) : originalUrl);
		return newUrl + '?ts=' + (new Date()).getTime()
	}

	function refreshInsuranceCardImage(patientInsuranceProfile) {
		
		var frontImage = (patientInsuranceProfile.frontImage == null) ? "" : patientInsuranceProfile.frontImage;
		var frontImage2 = (patientInsuranceProfile.frontImage2 == null) ? "" : patientInsuranceProfile.frontImage2;
		var frontImage3 = (patientInsuranceProfile.frontImage3 == null) ? "" : patientInsuranceProfile.frontImage3;
		var backImage = (patientInsuranceProfile.backImage == null) ? "" : patientInsuranceProfile.backImage;
		var backImage2 = (patientInsuranceProfile.backImage2 == null) ? "" : patientInsuranceProfile.backImage2;
		var backImage3 = (patientInsuranceProfile.backImage3 == null) ? "" : patientInsuranceProfile.backImage3;

		$("#primary_front").attr('src', (frontImage.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + frontImage + ".img") : '/portal/images/insurance-front.png');
		$("#secondary_front").attr('src', (frontImage2.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + frontImage2 + ".img") : '/portal/images/insurance-front.png');
		$("#tertiary_front").attr('src', (frontImage3.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + frontImage3 + ".img") : '/portal/images/insurance-front.png');

		$("#primary_back").attr('src', (backImage.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + backImage + ".img") : '/portal/images/insurance-back.png');
		$("#secondary_back").attr('src', (backImage2.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + backImage2 + ".img") : '/portal/images/insurance-back.png');
		$("#tertiary_back").attr('src', (backImage3.length > 0) ? applyTimestamp("http://162.209.124.93:8888/Images/" + backImage3 + ".img") : '/portal/images/insurance-back.png');
	}

	function setPatientInsuranceInfo(patientId) {
		var patient = patientProfiles[patientId];
		var patientInsuranceProfile = patient.insuranceProfile;
		var patientUserProfile = patient.userProfile;
		$(".insuranceUserId").val(patientId);
		if (patientInsuranceProfile) {
			$("#patientName").val(patientUserProfile.name);
			
			
			$("#dentalInsuranceId").val(patientInsuranceProfile.dentalInsuranceId);
			$("#primaryInsuranceHolder").val(patientInsuranceProfile.primaryInsuranceHolder);
			$("#primaryDob").val(patientInsuranceProfile.primaryDob);
			$("#company1").val(patientInsuranceProfile.company1);
			$("#company2").val(patientInsuranceProfile.company2);
			$("#company3").val(patientInsuranceProfile.company3);
			$("#group_id1").val(patientInsuranceProfile.group_id1);
			$("#group_id2").val(patientInsuranceProfile.group_id2);
			$("#group_id3").val(patientInsuranceProfile.group_id3);
			$("#member_id1").val(patientInsuranceProfile.member_id1);
			$("#member_id2").val(patientInsuranceProfile.member_id2);
			$("#member_id3").val(patientInsuranceProfile.member_id3);
			refreshInsuranceCardImage(patientInsuranceProfile);
		} else {
			
				
			
			$("#dentalInsuranceId").val("");
			$("#primaryInsuranceHolder").val("");
			$("#primaryDob").val("");
			$("#company1").val("");
			$("#company2").val("");
			$("#company3").val("");
			$("#group_id1").val("");
			$("#group_id2").val("");
			$("#group_id3").val("");
			$("#member_id1").val("");
			$("#member_id2").val("");
			$("#member_id3").val("");
			refreshInsuranceCardImage({});
		}
	}
	$(document).ready(function() {
		defineInsuranceTabInsuranceCardPhotoUploadUIBehavior();
		defineInsuranceTabUploadCardPhoto();
		defineInsuranceInfoTabValidator();
		defineInsuranceTabSubmitHandler();
	});
</script>

