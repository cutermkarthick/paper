<div class="modal fade" id="appointment-modal-sm" tabindex="-1" role="dialog" aria-labelledby="myModalAppointmentLabel" aria-hidden="true">
	<div class="modal-dialog modal-sm">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
				<h4 class="modal-title" id="myModalAppointmentLabel">Actions</h4>
			</div>
