<form name="patientBriefViewFormToSendSurveyEmail" id="patientBriefViewFormToSendSurveyEmail" method="post" action="https://www.paperlessdentists.com/portal/survey/sendsurveyemail">
	<input type="hidden" name="quickViewUserId" id="quickViewUserId" value="23"> <input type="hidden" name="dentistId" value="2">
</form>

<script type="text/javascript">
	$(document).ready(function() {
		$("#btnDetailedView").on('click', function() {
			var patientId = $(this).attr("patientId");
			hideAllDivs();
			$("#patient_detailed_view").removeClass("hide");
			setPatientDetailedView(patientId);
			$('#dentistDashboardPatientDetailedViewTabs a:eq(0)').tab('show')
			tabHref = $("ul#dentistDashboardPatientDetailedViewTabs li.active a").attr("href")
			history.pushState(null, null, tabHref);
		})

		$('#btnBackToAllPatientsTable').on('click', function() {
			hideAllDivs();
			$('#dentist_dashboard').removeClass("hide");
			tabHref = $("ul#collapseOne li.active a").attr("href")
			history.pushState(null, null, tabHref);
		});

		$('#btnMedicalAlert').on('click', function() {
			//var patientId = $(this).attr("patientId"); <-- TODO: Kechit, this does not work
			var patientId = $('#quickViewUserId').val();
			hideAllDivs();
			$('#patient_detailed_view').removeClass("hide");
			setPatientDetailedView(patientId);
			$('#dentistDashboardPatientDetailedViewTabs a[href="#tabMEDICALHISTORY"]').tab('show');
			history.pushState(null, null, "#tabMEDICALHISTORY");
		});

		$('#btnRefer').on('click', function() {
			$("#refer-modal-lg").modal();
		});

		$('#btnDoneRefer').on('click', function() {
			$('#refer_form').submit();
		});

		var allDentists = JSON.parse("[{\"id\":1,\"name\":\"Dr. Rita  Mitbavkar\"},{\"id\":3,\"name\":\"Studio  Dental\"},{\"id\":4,\"name\":\"Dr. Bone  Bone\"},{\"id\":5,\"name\":\"Dr. Soniya  More\"},{\"id\":6,\"name\":\"Dr Fernando\\r\\n   Pe\u00F1alva\"},{\"id\":7,\"name\":\"Dr David   Li\"},{\"id\":8,\"name\":\"Dr Amol  Chavarkar\"},{\"id\":9,\"name\":\"Dr Tushar  Kothalkar\"},{\"id\":10,\"name\":\"Dr Akash  Malla\"},{\"id\":11,\"name\":\"Dr Poonam  Kamat\"},{\"id\":12,\"name\":\"Dr Pravir  Ramtekkar\"},{\"id\":13,\"name\":\"Dr. Daisy G Ison\"},{\"id\":14,\"name\":\"Dr. Michael   Wong\"},{\"id\":15,\"name\":\"Dr Steven E. Roth\"},{\"id\":16,\"name\":\"Dr. Sheetal  Gokhale\"},{\"id\":17,\"name\":\"Dr. Madhavi  Arumilli\"},{\"id\":18,\"name\":\"Dr Shawn   Cabbell\"},{\"id\":19,\"name\":\"Dr. Daniel T. Lee\"},{\"id\":20,\"name\":\"Dr Romy  Singh\"}]");
		setReferDentists(allDentists, $("#referDentistId"));

		// bind 'Registration Form' and provide a simple callback function 
		$('#refer_form').ajaxForm({
			success : function(responseText) {
				if (responseText.REASON) {
					setReferNotificationMessage(responseText.REASON);
				} else {
					setReferNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setReferNotificationMessage("Error occurred. Please try again later.");
			}
		});

	$('#btnSurveyEmail').on('click', function() {
			$('#patientBriefViewFormToSendSurveyEmail').submit();
		});

		$('#patientBriefViewFormToSendSurveyEmail').ajaxForm({
			success : function(responseText) {
				if (responseText.SUCCESS) {
					$('#surverySentStatus').html("Survey email sent successfully.");
				} else {
					$('#surverySentStatus').html("Error occurred while sending survey email. Please try again later.");
				}
			},
			error : function(responseText) {
				$('#surverySentStatus').html("Error occurred while sending survey email. Please try again later.");
			}
		});
	});

	var recommendKeys = {
		1 : "Not at all",
		2 : "Slightly likely",
		3 : "Moderately likely",
		4 : "Very likely",
		5 : "Extremely likely"
	};

	var satisfactionKeys = {
		1 : "Dissatisfied",
		2 : "Slightly satisfied",
		3 : "Somewhat satisfied",
		4 : "Very satisfied",
		5 : "Extremely satisfied"
	};

	var serviceKeys = {
		1 : "Incompetent",
		2 : "Less than average",
		3 : "Average",
		4 : "Very good",
		5 : "Excellent"
	};

	function getValueForKey(keysObj, keyToGet) {
		for (property in keysObj) {
			if (property == keyToGet) {
				return keysObj[property];
			}
		}
	}

	$('#btnSurveyResults').on(
			'click',
			function() {
				//var sJson = $('#quickSurveyResults').val();
				if ($('#quickSurveyResults').val() != "") {
					var surveyJson = JSON.parse($('#quickSurveyResults').val());
					var recommend = surveyJson.recommend;
					var satisfied = surveyJson.satisfied;
					var satisfied_treatment = surveyJson.satisfied_treatment;
					var service_provided = surveyJson.service_provided;
					var feedback = surveyJson.feedback;
					$('#myModalSurveyResult .modal-body #recommend_badge')
							.val(getValueForKey(recommendKeys, recommend));
					$('#myModalSurveyResult .modal-body #satisfied_badge').val(
							getValueForKey(satisfactionKeys, satisfied));
					$('#myModalSurveyResult .modal-body #satisfied_treatment_badge').val(
							getValueForKey(satisfactionKeys, satisfied_treatment));
					$('#myModalSurveyResult .modal-body #service_provided_badge').val(
							getValueForKey(recommendKeys, service_provided));
					$('#myModalSurveyResult .modal-body #survey_feedback').val(feedback);
				} else {
					$('#myModalSurveyResult .modal-body #recommend_badge').val("");
					$('#myModalSurveyResult .modal-body #satisfied_badge').val("");
					$('#myModalSurveyResult .modal-body #satisfied_treatment_badge').val("");
					$('#myModalSurveyResult .modal-body #service_provided_badge').val("");
					$('#myModalSurveyResult .modal-body #survey_feedback').val("");
				}

			});

	$("#myModalSurveyResult").on('hidden.bs.modal', function() {
		$('#myModalSurveyResult .modal-body #recommend_badge').val("");
		$('#myModalSurveyResult .modal-body #satisfied_badge').val("");
		$('#myModalSurveyResult .modal-body #satisfied_treatment_badge').val("");
		$('#myModalSurveyResult .modal-body #service_provided_badge').val("");
		$('#myModalSurveyResult .modal-body #survey_feedback').val("");
	});

	function setReferNotificationMessage(message) {
		var notificationDiv = $(".refer_notification")
		notificationDiv.removeClass("hide");
		notificationDiv.show();
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	function setReferDentists(allDentists, $select) {
		for ( var index in allDentists) {
			var dentist = allDentists[index];
			$select.append($("<option value='" + dentist.id + "'>" + dentist.name + " </option>"));
		}
	}

	function hasMedicalAlertCondition(patient_history) {
		for (condition in patient_history) {
			if (patient_history[condition] == "Submit") {
			} else if (patient_history[condition] == "yes" && condition != "good_health") {
				return true;
			} else if (patient_history[condition] == "no" && condition == "good_health") {
				return true;
			} else if (patient_history[condition] == condition && condition != "completed"  && condition != "chk_allergies") {
				return true;
			}
		}
		if ((patient_history["medication_list"] != null && patient_history["medication_list"] != "") || patient_history["other_allergic"]
				|| patient_history["anything_else"]) {
			return true;
		}
		return false;
	}

