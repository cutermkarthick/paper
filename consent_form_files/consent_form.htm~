<script>

		
	function setPatientDentalHistory(patientHistoryString) {
		var patient_dental_history = JSON.parse(patientHistoryString);
		for (condition in patient_dental_history) {
			if (patient_dental_history[condition] == "Submit") {
			} else if (patient_dental_history[condition] == "yes") {
				$("#" + condition + "_yes").attr("checked", "checked");
			} else if (patient_dental_history[condition] == "no") {
				$("#" + condition + "_no").attr("checked", "checked");
			} else if (patient_dental_history[condition] == condition) {
				$("#" + condition).attr("checked", true);
			}else {
				$("#anything_else").val(patient_dental_history[condition]);
				$("#" + condition).val(patient_dental_history[condition]);
			}
		}
	}
	
	

	function setDentalNotificationMessage(message) {
		var notificationDiv = $(".dental_notification")
		notificationDiv.removeClass("hide");
		notificationDiv.text(message);
		setTimeout(function() {
			notificationDiv.addClass("hide");
		}, 15000);
	}

	function defineDentalButtons() {
		$('#btnDoneDental').on('click', function() {
			// TODO:  Insert Validation code here.
			$('#patientForm_Dental').submit();
		});

	}

	

	$(document).ready(function() {
		defineDentalButtons();
		//setPatienDentalHistory(patientDentalProfile);
		//defineDentalHistoryTabValidator();

		$('#patientForm_Dental').ajaxForm({
			success : function(responseText) {
				if (responseText.SUCCESS) {
					if (responseText.REASON) {
						setDentalNotificationMessage(responseText.REASON);
					} else {
						setDentalNotificationMessage("Dental form has been updated successfully.");
					}
					//refreshes Dental and consent history
					var userId = $(".dental_userId").val();
					refreshPatient(userId);
				} else {
					setDentalNotificationMessage("Error occurred. Please try again later.");
				}
			},
			error : function(responseText) {
				setDentalNotificationMessage("Error occurred. Please try again later.");
			}
		});
		

	});




	function setPatientDentalInfo(patientId) {
		$("#patientForm_Dental input:text").val("");
		var patient = patientProfiles[patientId];
		var patientDentalProfile = patient.dentalprofile;
		if (patientDentalProfile) {
			//setPatientDentalHistory(JSON.stringify(patientDentalProfile), $("#patientForm_Dental"));
			setPatientDentalHistory(JSON.stringify(patientDentalProfile));
			$('#btnDoneDental').removeClass("disabled");
		
		} else {
			$("#patientForm_Dental input").attr("disabled", false)

		}
		$(".dental_userId").val(patientId);
		$(".dental_userId").attr("disabled", false);
	}
</script>
