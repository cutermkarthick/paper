										<script type="text/javascript">
											$(document).ready(
													function() {
														var textMaxChars = 256;
														var textLength = $('#remarks').val().length;
														var textRemaining = textMaxChars - textLength;
														$('#remarks_feedback').html(
																textRemaining + ' characters remaining');
														$('#remarks').keyup(
																function() {
																	var textLength = $('#remarks').val().length;
																	var textRemaining = textMaxChars - textLength;
																	$('#remarks_feedback').html(
																			textRemaining + ' characters remaining');
																});
